<a href="/" class="btn btn-primary coverPages">Terug naar overzigt</a>

<div class="content-container">
    <form action="#" method="post">
        <div class="row">
            <div class="col-lg">
                <p><label for="#">TCVT Nummer</label> <input type="number" min="0" class="form-control" value="{{TCVTNumber}}"></p>
                <p><label for="#">Keuringsdatum</label> <input type="text" class="form-control" value="{{inspectionDate}}"></p>
                <p><label for="#">Uitvoerder</label> <input type="text" class="form-control" value="{{executor}}"></p>
                <p><label for="#">Deskundige</label> <input type="text" class="form-control" value="{{specialist}}"></p>
                <p><label for="#">Opstelling kraan</label> <input type="text" class="form-control" value="{{crainSetup}}"></p>
                <p><label for="#">Uitvoering toren haakhoogte</label> <input type="text" class="form-control" value="{{executionTowerHookHeight}}"></p>
                <p><label for="#">Soort giek</label> <input type="text" class="form-control" value="{{boomType}}"></p>
                <p><label for="#">Teleskoopgiek delen</label> <input type="number" class="form-control" value="{{telescopicBoomParts}}"></p>
                <p><label for="#">Opbouwgiek meters</label> <input type="number" class="form-control" value="{{constructionBoomMeters}}"></p>
                <p><label for="#">hulpgiek meters</label> <input type="number" class="form-control" value="{{jibBoomMeters}}"></p>
                <p><label for="#">Fly jib delen</label> <input type="number" class="form-control" value="{{flyJibParts}}"></p>
                <p><label for="#">Gieklengte</label> <input type="number" class="form-control" value="{{BoomLength}}"></p>
            </div>
            <div class="col-lg">
                <p><label for="#">Topbaar</label> <input type="checkbox" class="form-control" {{topable}}></p>
                <p><label for="#">Loopkat</label> <input type="checkbox" class="form-control" {{trolley}}></p>
                <p><label for="#">Verstelbare giek</label> <input type="checkbox" class="form-control" {{adjustableBoom}}></p>
                <p><label for="#">Soort stempels</label> <input type="checkbox" class="form-control" {{stampsType}}></p>
                <p>
                    <label for="#">Tekortkomingen</label>
                    <div class="row">
                        <div class="col">
                            <p>Ja: <input type="radio" name="shortcomings" class="form-control" {{shortcomings_yes}}></p>
                        </div>
                        <div class="col">
                            <p>Nee: <input type="radio" name="shortcomings" class="form-control" {{shortcomings_no}}></p>
                        </div>
                    </div>
                </p>
                <p><label for="#">Afgemeeld voor</label> <input type="date" class="form-control" value="{{signOutBefore}}"></p>
                <p>
                    <label for="#">Toelichting</label>
                    <textarea name="" rows="10" class="form-control">{{elucidation}}</textarea>
                </p>
                <p><label for="#">Werkinstructie</label> <input type="text" class="form-control" value="{{workInstruction}}"></p>
                <p><label for="#">Kabelleverancier</label> <input type="text" class="form-control" value="{{cableSupplier}}"></p>
                <p><label for="#">Waarmemingen</label> <input type="text" class="form-control" value="{{observations}}"></p>
                <p></p>
                <p><label for="#">Aantal bedrijfsuren</label> <input type="number" min="0" class="form-control" value="{{operatingHours}}"></p>
                <p><label for="#">Aflef redenen</label> <input type="text" class="form-control" value="{{discardReason}}"></p>
            </div>
        </div>
        <input type="submit" value="Opslaan" class="btn btn-success btn-block">
    </form>
</div>